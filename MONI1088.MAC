UPTAS4: LD   A,48H        ;Zeitschleife
UPTAS5: DEC  A 
        JR   NZ,UPTAS5 
        IN   A,(P04)      ;PIO1 Port A lesen
        BIT  7,A          ;Taste gedrueckt?
        RES  7,A          ;7. Bit ruecksetzen
        RET 
